@model IEnumerable<EDSU_SYSTEM.Models.CourseAllocation>

@{
    ViewData["Title"] = "Index";
}

<div id="PlaceHolderHere"></div>
<div class="main-container" id="container">
    <partial name="_StaffDashboardPartial" />
    <!--  BEGIN CONTENT AREA  -->
    <div id="content" class="main-content">
        <div class="layout-px-spacing">
            <div class="row layout-spacing">
                <!-- Content -->
                <div class="col-xl-10 col-lg-10 col-md-10 col-sm-12 mx-auto layout-top-spacing">
                    <div class="widget-content widget-content-area br-6">
                        <table id="html5-extension" class="table table-hover non-hover" style="width:100%">
                            <thead>
                                <tr>
                                    <th>Title</th>
                                    <th>Code</th>
                                    <th>Level</th>
                                    <th>Semester</th>
                                    <th>Credit Unit</th>
                                    <th>Status</th>
                                    <th>Department</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Courses.Title)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Courses.Code)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Courses.Levels.Name)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Courses.Semesters.Name)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Courses.CreditUnit)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Courses.Status)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Courses.Departments.Name)
                                        </td>
                                        <td>
                                            @*<a asp-action="EnrollStudent" asp-controller="Canvas" asp-route-id="@item.SchoolEmailAddress" class="bs-tooltip" data-toggle="tooltip" data-placement="top" title="" data-original-title="Enroll to LMS">
                                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-edit-2 p-1 br-6 mb-1"><path d="M17 3a2.828 2.828 0 1 1 4 4L7.5 20.5 2 22l1.5-5.5L17 3z"></path></svg>
                                            </a>*@
                                        </td>
                                        
                                    </tr>
                                }
                            </tbody>
                        </table>
                       
                    </div>

                </div>
                
            </div>
        </div>
    </div>
    <!--  END CONTENT AREA  -->
</div>
<!-- END MAIN CONTAINER -->